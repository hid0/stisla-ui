{
    "name": "hid-dev/stisla-ui",
    "description": "Laravel UI with Stisla Free Bootstrap 4 Admin Template ",
    "keywords": ["laravel", "ui", "stisla"],
    "license": "MIT",
    "authors": [
        {
            "name": "Faiz Hidayatulloh",
            "email": "faiz14hiedayatoelloh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^7.2.5",
        "laravel/ui": "^1.0|^2.0",
        "illuminate/console": "~5.8|^6.0|^7.0",
        "illuminate/filesystem": "~5.8|^6.0|^7.0",
        "illuminate/support": "~5.8|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "HidDev\\LaravelUiStisla\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "HidDev\\LaravelUiStisla\\StislaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
